!!!
html(ng-app="frisb")
  head
    meta(charset='utf8')
    base(href='/')
    title FRISB
    script(src='js/less-1.3.3.min.js')
    link(rel='stylesheet', href='/css/frisb.less')
    link(rel='stylesheet', href='/css/bootstrap-responsive.css')
    link(rel='stylesheet', href='/css/bootstrap.css')
    
block body
  div.container-fluid
    header.row-fluid
      div.navbar
        nav.navbar-inner
          a.brand(href='#') FRISB
          ul.nav
            li.active
              a(href='#') Home
            li
              a(href='/skills') Skills
            li
              a(href='/users') Users
            li
              a(href='/teams') Teams
    main.row-fluid
      aside.span4#notes-menu
        div#todo-list
          h3 ToDo List
          ol
            input(type="checkbox") todo item
            input(type="checkbox") todo item
        div#markup-menu
          h3 Mark Up Tools
          ul
            li 
              input(type="checkbox", id="show", value="Show Mark Up") 
              label(for="show") Show Mark Up
            li
              a.todo-list Add a ToDo
            li
              a.red-pen Mark up with red pen
            li
              a.replace-text Replace Text
      div.span8
        h1 FRISB
        p Welcom to FRISB

  script(src="http://code.jquery.com/jquery-1.10.0.min.js")
  script(src='js/lib/angular/angular.js')
  script(src='js/app.js')
  script(src='js/services.js')
  script(src='js/controllers.js')
  script(src='js/filters.js')
  script(src='js/directives.js')
  script(src='js/bootstrap.js')

  
